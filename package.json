{
  "name": "fulldev-blocks",
  "version": "0.1.0-experimental.15",
  "description": "A block library compatible with fulldev-ui",
  "homepage": "https://github.com/fulldotdev/blocks",
  "author": "Fulldev <contact@full.dev> (https://full.dev)",
  "repository": "github:fulldotdev/blocks",
  "bugs": "https://github.com/fulldotdev/blocks/issues",
  "scripts": {
    "dev": "astro dev",
    "test": "astro check",
    "check": "astro check",
    "build": "astro build",
    "connect": "pnpm link --global && pnpm link --global fulldev-ui"
  },
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./*": "./src/*",
    "./integration": "./src/integration/integration.ts",
    "./404.astro": "./src/pages/404.astro",
    "./contact.astro": "./src/pages/contact.astro",
    "./[...page].astro": "./src/pages/[...page].astro",
    "./[categories]/[...category].astro": "./src/pages/[categories]/[...category].astro",
    "./[categories]/index.astro": "./src/pages/[categories]/index.astro",
    "./[posts]/[...post].astro": "./src/pages/[posts]/[...post].astro",
    "./[posts]/index.astro": "./src/pages/[posts]/index.astro",
    "./[products]/[...product].astro": "./src/pages/[products]/[...product].astro",
    "./[products]/index.astro": "./src/pages/[products]/index.astro"
  },
  "devDependencies": {
    "@astrojs/check": "^0.5.10",
    "astro": "^4.11.5",
    "fulldev-ui": "^0.3.35",
    "prettier": "3.2.5",
    "prettier-plugin-astro": "0.13.0",
    "prettier-plugin-astro-organize-imports": "^0.4.7",
    "prettier-plugin-css-order": "^2.1.2",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-svelte": "^3.2.3",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "@astrolib/seo": "1.0.0-beta.5",
    "@radix-ui/colors": "^3.0.0",
    "@rollup/plugin-yaml": "^4.1.2",
    "astro-seo": "^0.8.4",
    "bezier-easing": "^2.1.0",
    "colorjs.io": "^0.5.2",
    "deepmerge": "^4.3.1",
    "fs-extra": "^11.2.0",
    "js-yaml": "^4.1.0",
    "radash": "^12.1.0",
    "remeda": "^2.11.0",
    "vite-plugin-virtual": "^0.3.0"
  }
}
