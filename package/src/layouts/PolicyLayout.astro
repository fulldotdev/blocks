---
import type { ComponentProps } from 'astro/types'
import { getEntry } from 'astro:content'
import { Prose } from 'fulldev-ui'
import Layout from './Layout.astro'
import NavLayout from './NavLayout.astro'

interface Props extends ComponentProps<typeof Layout> {
  hero?: any
  blocks?: any
}

const { hero, blocks, ...rest } = Astro.props

const cta = await getEntry('globals', 'cta')
---

<NavLayout {...rest}>
  <Prose>
    <slot name="content" />
  </Prose>
</NavLayout>

<style is:global lang="scss"></style>
